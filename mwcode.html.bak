  /******* Hero Section *****/

.hero_sec {
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
    height: 600px;
}
.hero_rowed {
    display: flex;
}
.left_content {
    width: 50%;
}
.right_video {
    display: flex;
    justify-content: center;
    width: 50%;
    height: auto;
}
.right_video iframe {
    border-radius: 20px;
}
.left_content h1 {
  font-size: 45px;
  padding: 15px 0px;
  color: #00B79F;
  padding-top: 0;
  padding-right: 40px;
}
.left_content p {
  line-height: 1.3;
  font-size: 18px;
  padding-bottom: 20px;
  padding-right: 40px;
}

/* .rotate_image {
  transform: rotate(360deg);
} */
.main_rotate {
  position: relative;
  width: fit-content;
}
.rotate_image {
  width: fit-content;
}
.programmer_image {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.rotate_image img {
  animation: rotate 9s linear infinite;
}

@keyframes rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

/******* Logo carousel *******/

@keyframes slide {
  from {
    transform: translateX(0);
  }
  to {
    transform: translateX(-100%);
  }
}

.logos {
  overflow: hidden;
  padding: 40px 0;
  background: #00B79F;
  white-space: nowrap;
  position: relative;
}

.logos:before,
.logos:after {
  position: absolute;
  top: 0;
  width: 250px;
  height: 100%;
  content: "";
  z-index: 2;
}

.logos:before {
  left: 0;
  background: linear-gradient(to left, rgba(255, 255, 255, 0), #00B79F);
}

.logos:after {
  right: 0;
  background: linear-gradient(to right, rgba(255, 255, 255, 0), #00B79F);
}

.logos:hover .logos-slide {
  animation-play-state: paused;
}

.logos-slide {
  display: inline-block;
  animation: 15s slide infinite linear;
}

.logos-slide img {
  height: 50px;
  margin: 0 40px;
}

/****** overlap section ******/

.overlap_rowed {
  display: flex;
  align-items: center;
  gap: 50px;
  padding: 60px 0;
  position: relative;
}
.overlap_content {
  width: 50%;
}
.overlap_content h2 {
    font-size: 42px;
}
.right_slider {
  display: flex;
  align-items: center;
  width: 60%;
  position: relative;
}
.slider_inner {
  display: flex !important;
  align-items: center;
}
.imgbox {
  width: 50% !important;
}
.contentbox {
  /* position: relative; */
  width: 50% !important;
  background: #fff;
  max-width: 400px;
  padding: 25px;
  transform: translate(-15%, 0%);
  border-radius: 10px;
  border-left: 12px solid #00B79F;
  box-shadow: 1px 2px 5px 0px rgba(0,0,0,0.75);
}
.overlap_content h2 {
  font-size: 42px;
}
.overlap_content p {
  font-size: 18px;
  line-height: 25px;
  padding-bottom: 50px;
  padding-top: 10px;
}
.imgbox > img {
  width: 100%;
  border-radius: 10px;
}
.contentbox > img {
  padding: 10px 0;
}
.contentbox > p::before {
  line-height: 20px;
  content: '';
  width: 95px;
  height: 50px;
  background-image: url(./media/quote-left.png);
  background-size: 50px;
  background-repeat: no-repeat;
  background-position: center;
  position: absolute;
  top: -30px;
  left: 0;
  z-index: 1;
}
.slick-prev:before, .slick-next:before {
  display: none;
}
.slick_batn.slick-arrow {
  position: absolute;
  left: -63%;
  top: 64%;
  transform: translate(-63%, 64%);
}
.slick_batn .slick-prev {
  background-color: #00B79F;
  padding: 20px;
  border-radius: 50px;
}
.slick_batn .slick-prev:hover, .slick-prev:focus, .slick-next:hover, .slick-next:focus {
  color: #00B79F;
  outline: none;
  background: #00B79F;
}
.slick_nxt.slick-arrow {
  margin-inline: 65px;
}
.slick_batn > img {
  max-width: 45px;
  cursor: pointer;
}
.slick-dots li.slick-active button:before {
  opacity: 1 !important;
  color: #00B79F !important;
}
.slick-dots li button:before {
  font-size: 12px !important;
}
.slick-dots {
  bottom: -50px !important;
}
.slick-dots li button:hover {
  color: #00B79F !important;
}
.slick-dots li button:before {
  color: #00B79F !important;
}

/********* scroll images **************/

.scroll_rowed {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.scroll_image {
  background-image: url('./media/otaa.png');
