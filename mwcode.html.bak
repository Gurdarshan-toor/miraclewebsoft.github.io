}
/****** button **********/

.get_batn {
    padding: 0.1em 0.25em;
    width: 13em;
    height: 4.2em;
    background-color: #00B79F;
    border: 0.08em solid #fff;
    border-radius: 0.3em;
    font-size: 12px;
    cursor: pointer;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .get_batn a {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    bottom: 0.4em;
    width: 8.25em;
    height: 2.5em;
    background-color: #00B79F;
    border-radius: 0.2em;
    font-size: 1.5em;
    color: #fff;
    border: 0.08em solid #fff;
    box-shadow: 0 0.4em 0.1em 0.019em #fff;
    text-decoration: none;
  }
  .get_batn a:hover {
    transition: all 0.6s;
    transform: translate(0, 0.4em);
    box-shadow: 0 0 0 0 #fff;
  }
  .get_batn a:not(hover) {
    transition: all 1s;
  }

